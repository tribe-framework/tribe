# Project Configuration

## PROJECT_NAME cannot have spaces or uppercase
PROJECT_NAME="tribe"

## Config
DISPLAY_ERRORS=false
DEFAULT_TIMEZONE="Asia/Kolkata"

## Enter your domain.tld without http:// or https:// and without trailing slash
BARE_URL="localhost"

## Your email address for SSL, if required
CONTACT_EMAIL="your-name@your-collective.org"

## Database settings, make sure you change the passwords
DB_ROOT_PASSWORD="rootpassword"
DB_NAME="tribe_db"
DB_USER="tribe_user"
DB_PASS="userpassword"
DB_HOST="tribe_mysql"
DB_PORT=3306

## Tribe port
TRIBE_PORT=12000

## phpMyAdmin port
PHPMYADMIN_PORT=12001

## Junction settings
JUNCTION_PORT=12002
JUNCTION_SLUG="junction"
JUNCTION_PASSWORD="password"
TRIBE_API_URL="http://localhost:12000"
TRIBE_API_KEY=""
PLAUSIBLE_AUTH=""
PLAUSIBLE_DOMAIN=""
HIDE_POSTCODE_ATTRIBUTION="false"

## Static website (dist) port
DIST_PORT=12003

## Dynamic website (dist-php) port
DIST_PHP_PORT=12004

## Filebrowser port and password for username admin
FILEBROWSER_PORT=12005
FILEBROWSER_PASSWORD="filepassword"

## Typesense port and API key
TYPESENSE_PORT=12006
TYPESENSE_API_KEY="xyz"

# Tika Configuration
TIKA_PORT=12007

# Whisper Configuration
WHISPER_PORT=12008
# Available models: tiny, tiny.en, base, base.en, small, small.en, medium, medium.en, large-v1, large-v2, large-v3, large-v3-turbo
WHISPER_MODEL="base"